    "@thorswap-lib/toolbox-utxo": "workspace:*",
    "@thorswap-lib/walletconnect": "workspace:*",
    "@vitest/coverage-istanbul": "0.34.4",
    "ethers": "6.7.1",
    "vite": "4.4.9",
    "vite-plugin-top-level-await": "1.3.1",
    "vite-plugin-wasm": "3.2.2",
    "vitest": "0.34.4"
  },
  "eslintConfig": {
    "extends": "../../../internal/eslint-config-thorswap"
